{
  "name": "nickel-syntax",
  "displayName": "Nickel syntax",
  "description": "Syntax highlighting for the Nickel language",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "url": "https://github.com/kubukoz/vscode-nickel-syntax",
    "type": "git"
  },
  "license": "Apache 2.0",
  "publisher": "kubukoz",
  "contributes": {
    "languages": [
      {
        "id": "nickel",
        "aliases": [
          "Nickel",
          "nickel"
        ],
        "extensions": [
          ".ncl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nickel",
        "scopeName": "source.nickel",
        "path": "./syntaxes/nickel.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^2.6.6"
  }
}
